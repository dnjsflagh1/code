    <btn_serverVersion normal="version.png" pressed="version.png" over="version.png" pos="180,170" shown="true"/>  
    <!-- 标签按钮-end  -->
    <!-- 左下角按钮-start -->
		<btn_set normal="set.jpg" pressed="set_act.jpg" over="set_over.jpg" disabled="set_dis.jpg" pos="28,497"/>
		<btn_close normal="close.jpg" pressed="close_act.jpg" over="close_over.jpg" disabled="close.jpg" pos="28,497" />  <!--108,497-->
