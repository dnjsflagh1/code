    <btn_services normal="services.jpg" pressed="services_act.jpg" over="services_over.jpg" pos="213,120"/>
    <!-- 游戏论坛  -->
		<btn_forum normal="forum.jpg" pressed="forum_act.jpg" over="forum_over.jpg" pos="304,120"/>
