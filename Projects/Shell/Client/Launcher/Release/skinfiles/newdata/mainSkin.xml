    <btn_newsevent normal="newsevent.jpg" pressed="newsevent_act.jpg" over="newsevent_over.jpg" pos="116,170"/>
    <btn_gamenote normal="gamenote.jpg" pressed="gamenote_act.jpg" over="gamenote_over.jpg" pos="202,170"/>
