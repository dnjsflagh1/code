#ifndef _THIRDPARTYCHARACTERDATA_H
#define _THIRDPARTYCHARACTERDATA_H
//*****************************************************************************************************
#include "CharacterData.h"
//*****************************************************************************************************
namespace MG
{
    /******************************************************************************/
    //第三方角色基础属性
    /******************************************************************************/
    struct ThirdPartyCharacterData : public CharacterData
    {   
        //------------------------------------------------------------------------------------------------
        ThirdPartyCharacterData()
        {

        }
        //------------------------------------------------------------------------------------------------
        ThirdPartyCharacterData& operator=(const ThirdPartyCharacterData& A)
        {
            if (this == &A)
            {
                return *this;
            }

            CharacterData::operator =(A);

            return *this;
        }
    };
}
//*****************************************************************************************************
#endif
//*****************************************************************************************************