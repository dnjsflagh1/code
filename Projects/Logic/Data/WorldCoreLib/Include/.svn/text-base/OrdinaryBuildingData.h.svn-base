//*****************************************************************************************************
#ifndef _ORDINARYBUILDINGDATA_H
#define _ORDINARYBUILDINGDATA_H
//*****************************************************************************************************
#include "NoCharacterData.h"
//*****************************************************************************************************
namespace MG
{
    /******************************************************************************/
    //平时建筑属性
    /******************************************************************************/
    struct OrdinaryBuildingData : NoCharacterData
    {   

        ORDINARY_BUILDING_TYPE    buildingType    ;

        //------------------------------------------------------------------------------------------------
        OrdinaryBuildingData()
            :buildingType(OBLT_UNKNOWN)
        {

        }

        //------------------------------------------------------------------------------------------------
        OrdinaryBuildingData& operator=(const OrdinaryBuildingData& A)
        {
            if (this == &A)
            {
                return *this;
            }

            NoCharacterData::operator =(A);

            buildingType        =    A.buildingType;

            return *this;
        }
    };
}
//*****************************************************************************************************
#endif
//*****************************************************************************************************